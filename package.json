{
  "name": "mvc-react-demo",
  "version": "1.0.0",
  "description": "A React demo application showcasing Model-View-Controller architecture",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:format": "yarn lint && yarn format",
    "lint:format:check": "yarn lint && yarn format:check",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "5.18.0",
    "@mui/material": "5.18.0",
    "@tanstack/react-query": "4.40.1",
    "@tanstack/react-query-devtools": "4.40.1",
    "axios": "1.12.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.6.1",
    "@types/eslint": "9.6.1",
    "@types/jest": "29.5.14",
    "@types/node": "24.5.0",
    "@types/react": "17.0.88",
    "@types/react-dom": "17.0.26",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "@vitejs/plugin-react": "5.0.2",
    "eslint": "9.35.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-fixed-jsdom": "0.0.10",
    "jiti": "2.5.1",
    "jsdom": "27.0.0",
    "lint-staged": "16.1.6",
    "msw": "2.11.2",
    "prettier": "3.6.2",
    "ts-jest": "29.4.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "vite": "7.1.5"
  },
  "packageManager": "yarn@4.10.1",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css,scss}": [
      "prettier --write"
    ]
  }
}
